<Window x:Class="HCI_Manifestations.dialogs.ShowManifestationsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_Manifestations.dialogs"
        mc:Ignorable="d"
        Title="ShowManifestationsDialog" Height="300" Width="500">
    <Grid>
        <DataGrid x:Name="manifestationsGrid" ItemsSource="{Binding Path=manifestations}" IsReadOnly="True"/>
    </Grid>
</Window>
